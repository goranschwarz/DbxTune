<launch4jConfig>
  <dontWrapJar>true</dontWrapJar>
  <headerType>console</headerType>
<!-- <headerType>console|gui</headerType> -->
  <jar>lib/dbxtune.jar</jar>
  <outfile>dist/dbxtune.exe</outfile>
  <errTitle>DbxTune</errTitle>
  <cmdLine></cmdLine>
  <chdir>.</chdir>
  <priority>normal</priority>
  <downloadUrl>http://java.com/download</downloadUrl>
  <supportUrl>www.dbxtune.com</supportUrl>
  <customProcName>false</customProcName>
  <stayAlive>true</stayAlive>
  <manifest></manifest>
  <icon>C:\projects\dbxtune\src\com\dbxtune\images\asetune_icon_32.ico</icon>
  <classPath>
    <mainClass>com.dbxtune.AseTune</mainClass>
    <cp>.\classes\</cp>
    <cp>.\lib\dbxtune.jar</cp>
    <cp>.\lib\jconn4.jar</cp>
    <cp>.\lib\jconn3.jar</cp>
    <cp>.\lib\jtds-1.3.1.jar</cp>
    <cp>.\lib\dsparser.jar</cp>
    <cp>.\lib\log4j-1.2.17.jar</cp>
    <cp>.\lib\h2.jar</cp>
    <cp>.\lib\wizard.jar</cp>
    <cp>.\lib\miglayout-3.6.jar</cp>
    <cp>.\lib\swingx-all-1.6.5-1.jar</cp>
    <cp>.\lib\jchart2d-3.2.0.jar</cp>
    <cp>.\lib\planviewer.jar</cp>
    <cp>.\lib\commons-cli-1.2.jar</cp>
    <cp>.\lib\proxy-vole_20121203.jar</cp>
    <cp>.\lib\ganymed-ssh2-263.jar</cp>
  </classPath>
  <jre>
    <path></path>
    <minVersion>1.6.0</minVersion>
    <maxVersion></maxVersion>
    <jdkPreference>preferJre</jdkPreference>
    <initialHeapSize>16</initialHeapSize>
    <maxHeapSize>1024</maxHeapSize>
    <opt>-Dswing.defaultlaf=com.sun.java.swing.plaf.windows.WindowsLookAndFeel</opt>
    <opt>-Denv.sybase=&quot;%SYBASE%&quot;</opt>
    <opt>-Denv.dbxtune.home=&quot;%DBXTUNE_HOME%&quot;</opt>
    <opt>-Dxxx.launch4j.exedir="%EXEDIR%"</opt>
    <opt>-Dxxx.launch4j.exefile="%EXEFILE%"</opt>
    <opt>-Dxxx.env.path="%Path%"</opt>
    <opt>-Dxxx.settings="%HomeDrive%%HomePath%\\settings.ini"</opt>
  </jre>

<!--
  <splash>
    <file>src\com\dbxtune\images\asetune_splash.bmp</file>
    <waitForWindow>true</waitForWindow>
    <timeout>60</timeout>
    <timeoutErr>true</timeoutErr>
  </splash>
-->

  <versionInfo>
    <fileVersion>1.0.0.0</fileVersion>
    <txtFileVersion>1.0.0</txtFileVersion>
    <fileDescription>DbxTune - Performance Monitor for DBMS X</fileDescription>
    <copyright>(c)2010 Goran Schwarz. All Rights Reserved.</copyright>
    <productVersion>1.0.0.0</productVersion>
    <txtProductVersion>1.0.0</txtProductVersion>
    <productName>DbxTune - Performance Monitor for DBMS X</productName>
    <companyName>Goran Schwarz</companyName>
    <internalName>dbxtune</internalName>
    <originalFilename>dbxtune.exe</originalFilename>
  </versionInfo>
</launch4jConfig>